    <add name="conn" connectionString="Data Source=JOHNILIYA\JOHNILIYA;Initial Catalog=iStrawberryCampaing;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>-->
</configuration>
